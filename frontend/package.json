{
  "name": "performance-manager-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "start": "npm run build && cd ../backend && python3 start.py",
    "stop": "cd ../backend && python3 stop.py",
    "status": "cd ../backend && python3 stop.py status",
    "restart": "npm run stop && sleep 2 && npm run start"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "pinia": "^2.1.0",
    "sortablejs": "^1.15.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@types/sortablejs": "^1.15.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/ui": "^1.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "jsdom": "^23.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.0"
  }
}
